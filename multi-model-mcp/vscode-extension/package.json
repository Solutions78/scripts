{
  "name": "multi-model-mcp",
  "displayName": "Multi-Model MCP",
  "description": "Multi-model code assistant wired to an MCP server",
  "version": "0.1.1",
  "publisher": "modular-misfits",
  "engines": {
    "vscode": "^1.92.0"
  },
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "activationEvents": [
    "onView:misfitChat.view",
    "onView:misfitDashboard.view",
    "onCommand:multiModelMcp.openChat",
    "onCommand:multiModelMcp.openDashboard",
    "onCommand:multiModelMcp.refreshModels"
  ],
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "misfitContainer",
          "title": "Misfit MCP",
          "icon": "icon.png"
        }
      ]
    },
    "views": {
      "misfitContainer": [
        {
          "id": "misfitChat.view",
          "name": "Misfit Chat"
        },
        {
          "id": "misfitDashboard.view",
          "name": "Misfit Dashboard"
        }
      ]
    },
    "commands": [
      {
        "command": "multiModelMcp.openChat",
        "title": "Multi-Model: Open Misfit Chat"
      },
      {
        "command": "multiModelMcp.openDashboard",
        "title": "Multi-Model: Open Misfit Dashboard"
      },
      {
        "command": "multiModelMcp.refreshModels",
        "title": "Multi-Model: Refresh Models"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "multiModelMcp.refreshModels",
          "when": "view == misfitChat.view",
          "group": "navigation@1"
        }
      ]
    }
  },
  "categories": [
    "Other"
  ],
  "devDependencies": {
    "@types/node": "^24.9.1",
    "@types/vscode": "^1.92.0",
    "typescript": "^5.6.3"
  }
}
